package Com.nithya;
class std
{
	String name = "nithya";   
    int rollno = u9;
    
    std(){
    System.out.println("default constructor");
    }
}
public class todayy {
      
    public static void main(String[] args) {
        
         std s1 = new std();
         System.out.println("Name: " + s1.name);
         System.out.println("rollno: " + s1.rollno);
    }

}  


OUTPUT:
default constructor
Name: nithya
rollno: u9
